{
    "manifest_version": 3,
    "name": "Upwork Job Assistant",
    "version": "1.0.0",
    "description": "AI-driven resume assistant that analyzes and optimizes resumes for job market success",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "src/main.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}
